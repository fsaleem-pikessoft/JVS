{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/(.*\\.(css|js|html|ico|png|jpg|jpeg|gif|svg|webp))",
      "dest": "/$1"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/protection-plus",
      "dest": "/protection-plus.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/faqs",
      "dest": "/faqs.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}